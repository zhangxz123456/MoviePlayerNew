<Window x:Class="MoviePlayer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MoviePlayer"
        mc:Ignorable="d"
        Title="MoviePlayer Software" Height="700" Width="1000" WindowStartupLocation="CenterScreen" Icon="30001.ico">
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="MyStyle/MyStyle.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>
    <Grid Name="windowGrid">
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition Height="50"></RowDefinition>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="41"></ColumnDefinition>
            <ColumnDefinition MaxWidth="150"></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>

        <!--导航栏-->
        <Grid Grid.Row="0" Grid.Column="0" Background="#363636">
            <Grid.RowDefinitions>
                <RowDefinition Height="50"></RowDefinition>
                <RowDefinition Height="50"></RowDefinition>
                <RowDefinition Height="50"></RowDefinition>
                <RowDefinition Height="50"></RowDefinition>
                <RowDefinition Height="50"></RowDefinition>
                <RowDefinition></RowDefinition>
                <RowDefinition Height="50"></RowDefinition>
            </Grid.RowDefinitions>
            <RadioButton Name="rb1" Style="{DynamicResource RadioButtonStyle1}" Grid.Row="0" Tag="1"></RadioButton>
            <RadioButton Name="rb2" Style="{DynamicResource RadioButtonStyle2}" Grid.Row="1" Tag="2"></RadioButton>
            <RadioButton Name="rb3" Style="{DynamicResource RadioButtonStyle3}" Grid.Row="2" Tag="3" Height="50" VerticalAlignment="Top"></RadioButton>
            <RadioButton Name="rb4" Style="{DynamicResource RadioButtonStyle4}" Grid.Row="3" Tag="4"></RadioButton>
            <RadioButton Name="rb5" Style="{DynamicResource RadioButtonStyle5}" Grid.Row="4" Tag="5"></RadioButton>
            <RadioButton Name="rb6" Style="{DynamicResource RadioButtonStyle6}" Grid.Row="6" Tag="6"></RadioButton>
        </Grid>
        <!--播放操作栏-->
        <Grid Grid.Column="0" Grid.Row="1" Grid.ColumnSpan="3" Background="#4C4C4C">
            <Grid.ColumnDefinitions>
                <ColumnDefinition MaxWidth="200"></ColumnDefinition>
                <ColumnDefinition MaxWidth="150"></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition MaxWidth="120"></ColumnDefinition>
            </Grid.ColumnDefinitions>
            <!-- 播放按钮-->
            <Grid Grid.Column="0" Margin="0,0,10,0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition></ColumnDefinition>
                    <ColumnDefinition Width="35"></ColumnDefinition>
                    <ColumnDefinition Width="35"></ColumnDefinition>
                    <ColumnDefinition Width="35"></ColumnDefinition>
                    <ColumnDefinition Width="35"></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <Image Name="btnImgBack" Source="Resources/Icon_Back.png" Grid.Column="1" VerticalAlignment="Center" Width="30" Height="30" Tag="1" MouseDown="btnImgBack_MouseDown"></Image>
                <Image Name="btnImgPlay" Source="Resources/Icon_Play.png" Grid.Column="2" VerticalAlignment="Center" Width="30" Height="30" Tag="2" MouseDown="btnImgPlay_MouseDown"></Image>
                <Image Name="btnImgStop" Source="Resources/Icon_Stop.png" Grid.Column="3" VerticalAlignment="Center" Width="30" Height="30" Tag="3" MouseDown="btnImgStop_MouseDown"></Image>
                <Image Name="btnImgNext" Source="Resources/Icon_Forward.png" Grid.Column="4" VerticalAlignment="Center" Width="30" Height="30" Tag="4" MouseDown="btnImgNext_MouseDown"></Image>
            </Grid>

            <!--音量调节-->
            <Grid Grid.Column="1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition MaxWidth="25"></ColumnDefinition>
                    <ColumnDefinition></ColumnDefinition>
                    <ColumnDefinition MaxWidth="25"></ColumnDefinition>
                </Grid.ColumnDefinitions>

                <Image Name="btnImgVadd" Source="Resources/Icon_Vol_Add.png" Grid.Column="2" VerticalAlignment="Center" Width="20" Height="20" Tag="5"></Image>
                <Image Name="btnImgVReduce" Source="Resources/Icon_Vol_Reduce.png" Grid.Column="0" VerticalAlignment="Center" Width="20" Height="20" Tag="6"></Image>
                <Slider Name="sliderVol" Grid.Column="1" VerticalAlignment="Center" ValueChanged="sliderVol_ValueChanged" Maximum="1"></Slider>
            </Grid>

            <!--时间进度条-->
            <Slider Name="sliderTime" Grid.Column="2" VerticalAlignment="Center" Margin="10,0,5,0" ValueChanged="sliderTime_ValueChanged"></Slider>

            <!--显示时间-->
            <TextBlock Name="txtTime" Text="00:00:00/00:00:00" Grid.Column="3" VerticalAlignment="Center" Foreground="White" HorizontalAlignment="Right" Margin="0,0,5,0"></TextBlock>
        </Grid>
        <!--导航二级菜单-->
        <TabControl Name="tabControl" Grid.Column="1" Grid.Row="0" Background="#FF545454" Style="{DynamicResource TabControlStyle1}" >
            <!--采集-->
            <TabItem Name="tabItemF1" Style="{DynamicResource TabItemStyle1}">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition MaxHeight="30"></RowDefinition>
                        <RowDefinition MaxHeight="30"></RowDefinition>
                        <RowDefinition></RowDefinition>
                    </Grid.RowDefinitions>
                    <Button Name="btnLtcMode" Tag="1"  Background="#FF545454" Style="{DynamicResource ButtonStyle1}" Grid.Row="0" Click="btnLtcMode_Click">
                        <Button.Content>
                            <Grid Width="149" Height="30" x:Name="btnLtcModeGrid">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition MaxWidth="25"></ColumnDefinition>
                                    <ColumnDefinition></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <Image Source="Resources/Icon_M_LTC.png" Grid.Column="0" Margin="0,5,0,5" MaxHeight="18"></Image>
                                <TextBlock Name="txtLtcMode" Text="{DynamicResource txtLtcMode}" Foreground="White" Grid.Column="1" VerticalAlignment="Center"></TextBlock>
                            </Grid>
                        </Button.Content>
                    </Button>
                    <Button Name="btnLocalMode" Tag="2"  Background="#FF545454" Style="{DynamicResource ButtonStyle1}" Grid.Row="1" Height="30" VerticalAlignment="Bottom">
                        <Button.Content>
                            <Grid Width="149" Height="30" x:Name="btnLocModeGrid">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition MaxWidth="25" Width="25*"></ColumnDefinition>
                                    <ColumnDefinition Width="44*"></ColumnDefinition>
                                    <ColumnDefinition Width="80*"/>
                                </Grid.ColumnDefinitions>
                                <Image Source="Resources/Icon_MLocal.png" Grid.Column="0" Margin="0,5,0,5" MaxHeight="18"></Image>
                                <TextBlock Name="txtLocalMode" Text="{DynamicResource txtLocalMode}" Foreground="White" Grid.Column="1" VerticalAlignment="Center" Grid.ColumnSpan="2" Margin="0,7"></TextBlock>
                            </Grid>
                        </Button.Content>
                    </Button>
                </Grid>
            </TabItem>
            <!--关于-->
            <TabItem Name="tabItemF2" Style="{DynamicResource TabItemStyle1}">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="30"></RowDefinition>
                        <RowDefinition Height="30"></RowDefinition>
                        <RowDefinition Height="30"></RowDefinition>
                        <RowDefinition></RowDefinition>
                    </Grid.RowDefinitions>
                    <!--注册-->
                    <Button Name="btnRegister" Tag="1"  Background="#FF545454" Style="{DynamicResource ButtonStyle1}" Grid.Row="0">
                        <Button.Content>
                            <Grid Width="149" Height="30" x:Name="btnRegisterGrid">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition MaxWidth="30"></ColumnDefinition>
                                    <ColumnDefinition></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <Image Source="Resources/Icon_Resister.png" Grid.Column="0" Margin="0,5,0,5" MaxHeight="20"></Image>
                                <TextBlock Name="txtRegister" Text="{DynamicResource txtRegister}" Foreground="White" Grid.Column="1" VerticalAlignment="Center"></TextBlock>
                            </Grid>
                        </Button.Content>
                    </Button>
                    <!--更新日志-->
                    <Button Name="btnUpdateLog" Tag="2"  Background="#FF545454" Style="{DynamicResource ButtonStyle1}" Grid.Row="1">
                        <Button.Content>
                            <Grid Width="149" Height="30" x:Name="btnUpdateLogGrid">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition MaxWidth="30"></ColumnDefinition>
                                    <ColumnDefinition></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <Image Source="Resources/Icon_UpdateLog.png" Grid.Column="0" Margin="0,5,0,5" MaxHeight="15"></Image>
                                <TextBlock Name="txtUpdateLog" Text="{DynamicResource txtUpdateLog}" Foreground="White" Grid.Column="1" VerticalAlignment="Center"></TextBlock>
                            </Grid>
                        </Button.Content>
                    </Button>
                    <!--关于我们-->
                    <Button Name="btnAboutUs" Tag="3"  Background="#FF545454" Style="{DynamicResource ButtonStyle1}" Grid.Row="2">
                        <Button.Content>
                            <Grid Width="149" Height="30" x:Name="btnAboutUsGrid">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition MaxWidth="30"></ColumnDefinition>
                                    <ColumnDefinition></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <Image Source="Resources/Icon_AboutUs.png" Grid.Column="0" Margin="0,5,0,5" MaxHeight="15"></Image>
                                <TextBlock Name="txtAboutUs" Text="{DynamicResource txtAboutUs}" Foreground="White" Grid.Column="1" VerticalAlignment="Center"></TextBlock>
                            </Grid>
                        </Button.Content>
                    </Button>
                </Grid>
            </TabItem>
            <!--设置-->
            <TabItem Name="tabItemF3" Style="{DynamicResource TabItemStyle1}">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="30"></RowDefinition>
                        <RowDefinition Height="30"></RowDefinition>
                        <RowDefinition></RowDefinition>
                    </Grid.RowDefinitions>
                    <!--语言-->
                    <Button Name="btnLang" Tag="1"  Background="#FF545454" Style="{DynamicResource ButtonStyle1}" Grid.Row="0">
                        <Button.Content>
                            <Grid Width="149" x:Name="btnLangGrid">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition MaxWidth="30"></ColumnDefinition>
                                    <ColumnDefinition></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <Image Source="Resources/Icon_Language.png" Grid.Column="0" Margin="0,5,0,5" MaxHeight="17"></Image>
                                <TextBlock Name="txtLang" Text="语言(Language)" Foreground="White" Grid.Column="1" VerticalAlignment="Center"></TextBlock>
                            </Grid>
                        </Button.Content>
                    </Button>
                    <!--参数设置-->
                    <Button Name="btnParamSet" Tag="2"  Background="#FF545454" Style="{DynamicResource ButtonStyle1}" Grid.Row="1">
                        <Button.Content>
                            <Grid Width="149" x:Name="btnParamSetGrid">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition MaxWidth="30"></ColumnDefinition>
                                    <ColumnDefinition></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <Image Source="Resources/Icon_Set50X50.png" Grid.Column="0" Margin="0,5,0,5" MaxHeight="17"></Image>
                                <TextBlock Name="txtParamSet" Text="{DynamicResource txtParamSet}" Foreground="White" Grid.Column="1" VerticalAlignment="Center"></TextBlock>
                            </Grid>
                        </Button.Content>
                    </Button>
                </Grid>
            </TabItem>
        </TabControl>
        <!--内容-->
        <TabControl Name="tabControlShow" Background="#FF737373" Grid.Column="2" Grid.Row="0"  Style="{DynamicResource TabControlStyle2}" Margin="0,0,0,0">
            <!--播放-->
            <TabItem Name="tabItemFC1"  Style="{DynamicResource TabItemStyle2}">
                <TabControl Name="tabControlShowData" Background="#FF737373" Grid.Column="2" Grid.Row="0"  Style="{DynamicResource TabControlStyle2}">
                    <TabItem Name="tabItemFC11" Style="{DynamicResource TabItemStyle2}">
                        <!--播放界面-->
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="150"></ColumnDefinition>
                                <ColumnDefinition></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Grid Grid.Row="0" Grid.Column="0" Margin="-3" Background="#FF505050">
                                <Grid.RowDefinitions>
                                    <RowDefinition MaxHeight="130"/>
                                    <RowDefinition Height="20"/>
                                    <RowDefinition Height="1"/>
                                    <RowDefinition MaxHeight="80"/>
                                    <RowDefinition Height="1"/>
                                    <RowDefinition MaxHeight="80"/>
                                    <RowDefinition Height="1"/>
                                    <RowDefinition Height="30"/>
                                    <RowDefinition Height="1"/>
                                    <RowDefinition/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition/>
                                    <ColumnDefinition/>
                                    <ColumnDefinition/>
                                </Grid.ColumnDefinitions>
                                <ProgressBar x:Name="pb1" MaxWidth="25" Grid.Row="0" Grid.Column="0" Foreground="Red" Margin="5,5,5,5" Orientation="Vertical" Value="80" Maximum="255"/>
                                <ProgressBar x:Name="pb2" MaxWidth="25" Grid.Row="0" Grid.Column="1" Foreground="Green" Margin="5,5,5,5" Orientation="Vertical" Value="80" Maximum="255"/>
                                <ProgressBar x:Name="pb3" MaxWidth="25" Grid.Row="0" Grid.Column="2" Foreground="DodgerBlue" Margin="5,5,5,5" Orientation="Vertical" Value="80" Maximum="255"/>
                                <TextBlock x:Name="txtPbVal1" Grid.Row="1" Grid.Column="0" Text="255" HorizontalAlignment="Center" Foreground="White" VerticalAlignment="Center"/>
                                <TextBlock x:Name="txtPbVal2" Grid.Row="1" Grid.Column="1" Text="255" HorizontalAlignment="Center" Foreground="White" VerticalAlignment="Center"/>
                                <TextBlock x:Name="txtPbVal3" Grid.Row="1" Grid.Column="2" Text="255" HorizontalAlignment="Center" Foreground="White" VerticalAlignment="Center"/>

                                <!--环境特效显示-->
                                <TextBlock Background="#FF737373"  Grid.Row="2" Grid.ColumnSpan="3"/>
                                <Grid Grid.Row="3" Grid.ColumnSpan="3">
                                    <Grid.RowDefinitions>
                                        <RowDefinition MaxHeight="15"/>
                                        <RowDefinition/>
                                    </Grid.RowDefinitions>
                                    <TextBlock x:Name="txtGb1" Text="{DynamicResource txtEvEffect}" Foreground="White" Grid.Row="0" Margin="5,0,0,0"/>
                                    <Grid Grid.Row="1">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition/>
                                            <ColumnDefinition/>
                                            <ColumnDefinition/>
                                            <ColumnDefinition/>
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition/>
                                            <RowDefinition/>
                                        </Grid.RowDefinitions>
                                        <CheckBox x:Name="cbEv1" Grid.Row="0" Grid.Column="0" Foreground="White" Width="30" Height="25" VerticalAlignment="Center" Style="{DynamicResource EffectCheckBoxStyle}">
                                            <Grid Width="23" Height="23" Margin="0,2,0,0">
                                                <Image Source="/Resources/Icon_Effect_Lighting.png" Stretch="Fill"/>
                                            </Grid>
                                        </CheckBox>
                                        <CheckBox x:Name="cbEv2" Grid.Row="0" Grid.Column="1" Foreground="White" Width="30" Height="25"  VerticalAlignment="Center" Style="{DynamicResource EffectCheckBoxStyle}">
                                            <Grid Width="23" Height="23" Margin="0,2,0,0">
                                                <Image Source="/Resources/Icon_Effect_Wind50x50.png" Stretch="Fill"/>
                                            </Grid>
                                        </CheckBox>
                                        <CheckBox x:Name="cbEv3" Grid.Row="0" Grid.Column="2" Foreground="White" Width="30" Height="25"  VerticalAlignment="Center" Style="{DynamicResource EffectCheckBoxStyle}">
                                            <Grid Width="23" Height="23" Margin="0,2,0,0">
                                                <Image Source="/Resources/Icon_Effect_Bubble.png" Stretch="Fill"/>
                                            </Grid>
                                        </CheckBox>
                                        <CheckBox x:Name="cbEv4" Grid.Row="0" Grid.Column="3" Foreground="White" Width="30" Height="25" VerticalAlignment="Center" Style="{DynamicResource EffectCheckBoxStyle}">
                                            <Grid Width="23" Height="23" Margin="0,2,0,0">
                                                <Image Source="/Resources/Icon_Effect_Fog.png" Stretch="Fill"/>
                                            </Grid>
                                        </CheckBox>
                                        <CheckBox x:Name="cbEv5" Grid.Row="1" Grid.Column="0" Foreground="White" Width="30" Height="25"  VerticalAlignment="Center" Style="{DynamicResource EffectCheckBoxStyle}">
                                            <Grid Width="23" Height="23" HorizontalAlignment="Center">
                                                <Image Source="/Resources/Icon_Effect_Fire.png" Stretch="Fill"/>
                                            </Grid>
                                        </CheckBox>
                                        <CheckBox x:Name="cbEv6" Grid.Row="1" Grid.Column="1" Foreground="White" Width="30" Height="25" VerticalAlignment="Center" Style="{DynamicResource EffectCheckBoxStyle}">
                                            <Grid Width="23" Height="23" Margin="0,2,0,0">
                                                <Image Source="/Resources/Icon_Effect_Snow.png" Stretch="Fill"/>
                                            </Grid>
                                        </CheckBox>
                                        <CheckBox x:Name="cbEv7" Grid.Row="1" Grid.Column="2" Foreground="White" Width="30" Height="25" VerticalAlignment="Center" Style="{DynamicResource EffectCheckBoxStyle}">
                                            <Grid Width="23" Height="23" Margin="0,2,0,0">
                                                <Image Source="/Resources/Icon_Effect_Laser.png" Stretch="Fill"/>
                                            </Grid>
                                        </CheckBox>
                                        <CheckBox x:Name="cbEv8" Grid.Row="1" Grid.Column="3" Foreground="White" Width="30" Height="25" VerticalAlignment="Center" Style="{DynamicResource EffectCheckBoxStyle}">
                                            <Grid Width="23" Height="23" Margin="0,2,0,0">
                                                <Image Source="/Resources/Icon_Effect_Rain.png" Stretch="Fill"/>
                                            </Grid>
                                        </CheckBox>
                                    </Grid>
                                </Grid>

                                <!--座椅特效显示-->
                                <TextBlock Background="#FF737373"  Grid.Row="4" Grid.ColumnSpan="3"/>
                                <Grid Grid.Row="5" Grid.ColumnSpan="3">
                                    <Grid.RowDefinitions>
                                        <RowDefinition MaxHeight="15"/>
                                        <RowDefinition/>
                                    </Grid.RowDefinitions>
                                    <TextBlock x:Name="txtGb2" Text="{DynamicResource txtChairEffect}" Foreground="White" Grid.Row="0" Margin="5,0,0,0"/>
                                    <Grid Grid.Row="1">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition/>
                                            <ColumnDefinition/>
                                            <ColumnDefinition/>
                                            <ColumnDefinition/>
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition/>
                                            <RowDefinition/>
                                        </Grid.RowDefinitions>
                                        <CheckBox x:Name="cbCv1" Grid.Row="0" Grid.Column="0" Foreground="White" VerticalAlignment="Center" Width="30" Height="25" Style="{DynamicResource EffectCheckBoxStyle}">
                                            <Grid Width="23" Height="23" Margin="0,2,0,0">
                                                <Image Source="/Resources/Icon_Effect_Smell.png" Stretch="Fill"/>
                                            </Grid>
                                        </CheckBox>
                                        <CheckBox x:Name="cbCv2" Grid.Row="0" Grid.Column="1" Foreground="White" VerticalAlignment="Center" Width="30" Height="25" Style="{DynamicResource EffectCheckBoxStyle}">
                                            <Grid Width="23" Height="23" Margin="0,2,0,0">
                                                <Image Source="/Resources/Icon_Effect_Vibration.png" Stretch="Fill"/>
                                            </Grid>
                                        </CheckBox>
                                        <CheckBox x:Name="cbCv3" Grid.Row="0" Grid.Column="2" Foreground="White" VerticalAlignment="Center" Width="30" Height="25" Style="{DynamicResource EffectCheckBoxStyle}">
                                            <Grid Width="23" Height="23" Margin="0,2,0,0">
                                                <Image Source="/Resources/Icon_Effect_Leg.png" Stretch="Fill"/>
                                            </Grid>
                                        </CheckBox>
                                        <CheckBox x:Name="cbCv4" Grid.Row="0" Grid.Column="3" Foreground="White" VerticalAlignment="Center" Width="30" Height="25" Style="{DynamicResource EffectCheckBoxStyle}">
                                            <Grid Width="23" Height="23" Margin="0,2,0,0">
                                                <Image Source="/Resources/Icon_Effect_Water.png" Stretch="Fill"/>
                                            </Grid>
                                        </CheckBox>
                                        <CheckBox x:Name="cbCv5" Grid.Row="1" Grid.Column="0" Foreground="White" VerticalAlignment="Center" Width="30" Height="25" Style="{DynamicResource EffectCheckBoxStyle}">
                                            <Grid Width="23" Height="23" Margin="0,2,0,0">
                                                <Image Source="/Resources/Icon_Effect_Air.png" Stretch="Fill"/>
                                            </Grid>
                                        </CheckBox>
                                        <CheckBox x:Name="cbCv6" Grid.Row="1" Grid.Column="1" Foreground="White" VerticalAlignment="Center" Width="30" Height="25" Style="{DynamicResource EffectCheckBoxStyle}">
                                            <Grid Width="23" Height="23" Margin="0,2,0,0">
                                                <Image Source="/Resources/Icon_Effect_Push.png" Stretch="Fill"/>
                                            </Grid>
                                        </CheckBox>
                                        <CheckBox x:Name="cbCv7" Grid.Row="1" Grid.Column="2" Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Center" Width="30" Height="25" Style="{DynamicResource EffectCheckBoxStyle}">
                                            <Grid Width="23" Height="23" Margin="0,2,0,0">
                                                <Image Source="/Resources/Icon_CA.png" Stretch="Fill"/>
                                            </Grid>
                                        </CheckBox>
                                        <CheckBox x:Name="cbCv8" Grid.Row="1" Grid.Column="3" Foreground="White" VerticalAlignment="Center" Width="30" Height="25" Style="{DynamicResource EffectCheckBoxStyle}">
                                            <Grid Width="23" Height="23" Margin="0,2,0,0">
                                                <Image Source="/Resources/Icon_CB.png" Stretch="Fill"/>
                                            </Grid>
                                        </CheckBox>
                                    </Grid>
                                </Grid>

                                <!--导入影片-->
                                <Button x:Name="btnImport" Grid.Row="7" Grid.ColumnSpan="3" Style="{DynamicResource FilletButton}" Margin="10,2,10,3" Content="{DynamicResource btnImport}" Foreground="White" Click="btnImport_Click"/>
                                <TextBlock Background="#FF737373"  Grid.Row="6" Grid.ColumnSpan="3"/>
                                <TextBlock Background="#FF737373"  Grid.Row="8" Grid.ColumnSpan="3"/>
                                <ListView x:Name="ListView" Grid.Row="9" Grid.ColumnSpan="3" Margin="0,0,0,0" Background="#FF505050" BorderThickness="0" PreviewMouseDoubleClick="ListView_PreviewMouseDoubleClick">
                                    <ListView.ContextMenu>
                                        <ContextMenu Background="#FF545454">
                                            <MenuItem x:Name="listMenuPlay" Header="播放" Foreground="White" Tag="1" >
                                            </MenuItem>
                                            <MenuItem x:Name="listMenuAdd" Header="添加" Foreground="White" Tag="2" >
                                            </MenuItem>
                                            <MenuItem x:Name="listMenuMode" Header="播放模式" Foreground="White" >
                                                <MenuItem x:Name="listSubBalance1" Header="单个循环" Tag="1" Foreground="White" >
                                                </MenuItem>
                                                <MenuItem x:Name="listSubBalance2" Header="默认播放" Tag="2" Foreground="White" >
                                                </MenuItem>
                                                <MenuItem x:Name="listSubBalance3" Header="列表循环" Tag="3" Foreground="White" >
                                                    <MenuItem.Icon>
                                                        <Image x:Name="imgListSubBalance3"/>
                                                    </MenuItem.Icon>
                                                </MenuItem>
                                            </MenuItem>
                                            <MenuItem x:Name="listMenuDel" Header="清空" Foreground="White"  Tag="3" >
                                            </MenuItem>
                                            <MenuItem x:Name="listMenuDelAll" Header="清空所有" Foreground="White"  Tag="4">
                                            </MenuItem>
                                        </ContextMenu>
                                    </ListView.ContextMenu>
                                </ListView>
                            </Grid>
                        </Grid>
                    </TabItem>
                    <TabItem Name="tabItemFC12"  Style="{DynamicResource TabItemStyle2}">
                        <Grid>
                            <Label>11</Label>
                        </Grid>
                    </TabItem>
                </TabControl>
            </TabItem>
            <!--调试-->
            <TabItem Name="tabItemFC2"  Style="{DynamicResource TabItemStyle2}" BorderThickness="1" Unloaded="tabItemFC2_Unloaded">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition />
                        <ColumnDefinition />
                        <ColumnDefinition />
                        <ColumnDefinition />
                        <ColumnDefinition />
                        <ColumnDefinition />
                    </Grid.ColumnDefinitions>
                    <GridSplitter Width="2" Background="#FF626262" Grid.RowSpan="5" Grid.Row="0" Grid.Column="1" VerticalAlignment="Stretch" HorizontalAlignment="Right"></GridSplitter>
                    <GridSplitter Width="2" Background="#FF626262" Grid.RowSpan="5" Grid.Row="0" Grid.Column="3" VerticalAlignment="Stretch" HorizontalAlignment="Right"></GridSplitter>

                    <Label x:Name="label1" Content="{DynamicResource label1}"  Grid.Row="0" Grid.ColumnSpan="2" Foreground="White" FontSize="30" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Background="#FF545454" />
                    <Label x:Name="label2" Content="{DynamicResource label2}" Grid.Column="2"  Grid.Row="0" Grid.ColumnSpan="2" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="30" Foreground="White" Background="#FF3B3B3B" />
                    <Label x:Name="label3" Content="{DynamicResource label3}" Grid.Column="4" HorizontalContentAlignment="Center"  Grid.Row="0" VerticalContentAlignment="Center" FontSize="30" Grid.ColumnSpan="2"  Foreground="White" Background="#FF303030"/>
                    <Button x:Name="btnUp" Style="{DynamicResource FilletButton}" Margin="30,20,30,20"  Grid.Row="1" Grid.Column="0" VerticalContentAlignment="Center" Foreground="White" HorizontalContentAlignment="Center" Background="#FF939393" FontSize="36" Click="btnUp_Click">
                        <Button.Content>
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition></RowDefinition>
                                    <RowDefinition></RowDefinition>
                                </Grid.RowDefinitions>
                                <Image Source="Resources/Icon_Up.png" Grid.Row="0"></Image>
                                <Label Name="labUp" Content="{DynamicResource labUp}" Grid.Row="1" Foreground="White" VerticalContentAlignment="Center" HorizontalAlignment="Center" FontSize="15"></Label>
                            </Grid>
                        </Button.Content>
                    </Button>
                    <Button x:Name="btnDown"  Style="{DynamicResource FilletButton}" Margin="30,20,30,20" Grid.Row="1" Grid.Column="1"  Foreground="White"  Background="#FF939393" FontSize="36" Click="btnDown_Click">
                        <Button.Content>
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition></RowDefinition>
